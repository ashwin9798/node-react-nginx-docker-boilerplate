{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "136699605217.dkr.ecr.us-west-2.amazonaws.com/test-client",
      "hostname": "client",
      "essential": false,
      "memory": 256
    },
    {
      "name": "api",
      "image": "136699605217.dkr.ecr.us-west-2.amazonaws.com/test-api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "136699605217.dkr.ecr.us-west-2.amazonaws.com/test-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "api"],
      "memory": 64
    }
  ]
}
